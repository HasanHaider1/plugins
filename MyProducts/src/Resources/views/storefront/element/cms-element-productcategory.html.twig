{% block element_productcategory %}
    <div class="cms-element-productcategory" style="height: 100%; width: 100%">

        {% block element_productcategory_image_inner %}
            <div class="cms-el-productcategory">
                <div id="carouselExampleSlidesOnly" class="carousel slide" data-ride="carousel">
                    <div class="carousel-inner">
                        <div class="carousel-item active">
                            <img class="d-block w-100" src="/administration/static/img/cms/{{ element.config.data.value[0] }}" alt="First slide">
                        </div>
                        <div class="carousel-item">
                            <img class="d-block w-100" src="" alt="Second slide">
                        </div>
                        <div class="carousel-item">
                            <img class="d-block w-100" src="/administration/static/img/cms/preview_glasses_large.jpg" alt="Third slide">
                        </div>
                    </div>
                </div>
            </div>
        {% endblock %}
    </div>
{% endblock %}